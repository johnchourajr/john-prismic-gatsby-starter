{
  "Main": {
    "uid": {
      "type": "UID",
      "config": {
        "label": "slug",
        "placeholder": "URL of the post"
      }
    },
    "title": {
      "type": "Text",
      "config": {
        "label": "title",
        "placeholder": "Title of the page"
      }
    },
    "template": {
      "type": "Select",
      "config": {
        "options": ["page-template", "home-template"],
        "default_value": "page-template",
        "label": "Template"
      }
    },
    "nav_order": {
      "type": "Number",
      "config": {
        "label": "Nav Order"
      }
    },
    "top_level_page": {
      "type": "Boolean",
      "config": {
        "placeholder_false": "no",
        "placeholder_true": "yes",
        "default_value": true,
        "label": "Top Level Page"
      }
    },
    "hide_in_nav": {
      "type": "Boolean",
      "config": {
        "placeholder_false": "no",
        "placeholder_true": "yes",
        "default_value": false,
        "label": "Hide In Nav"
      }
    },
    "headline": {
      "type": "Text",
      "config": {
        "label": "Headline"
      }
    },
    "body": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph, preformatted, heading1, heading2, heading3, heading4, heading5, heading6, strong, em, hyperlink, image, embed, list-item, o-list-item, rtl",
        "allowTargetBlank": true,
        "label": "Body"
      }
    }
  }
}
