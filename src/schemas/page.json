        "label": "Template"
      }
    },
    "nav_order": {
      "type": "Number",
      "config": {
        "label": "Nav Order"
      }
    },
    "top_level_page": {
      "type": "Boolean",
      "config": {
        "placeholder_false": "no",
        "placeholder_true": "yes",
        "default_value": true,
        "label": "Top Level Page"
      }
    },
    "hide_in_nav": {
      "type": "Boolean",
      "config": {
        "placeholder_false": "no",
        "placeholder_true": "yes",
        "default_value": false,
        "label": "Hide In Nav"
