      "type": "UID",
      "config": {
        "label": "slug",
        "placeholder": "URL of the post"
      }
    },
    "title": {
      "type": "Text",
      "config": {
        "label": "title",
        "placeholder": "Title of the page"
      }
    },
    "template": {
      "type": "Select",
      "config": {
        "options": [
          "page-template",
          "home-template"
